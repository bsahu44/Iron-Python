<SCRIPT type=text/javascript>

$(function() {
//$('.countryLabel').css( "background-color", "" );
});

//$(".country").animate({width:'600px'}, 300);

$( ".menu" ).click(function() {  
$( this ).toggleClass( "active" );
if($(this).hasClass("active"))
{
$("#panel").animate({left:'0px'}, 300);
}
else
{
$("#panel").animate({left:'-200px'}, 300);
}


});





var oldValue=0
 transferValue = function(){
 
   //get value from calcVal spotfire Calculated Value Dyn Item
    value=$("#calcVal3").text().trim()
value1=$("#calcVal3").length
 
   //update input when oldValue changes
    if(value!=oldValue){
       $("#inputPropertyControl3 input").val(value).blur();

    }
if(value=="All Recs"){
  $("#BSC1").show();   
 $("#BSC2").hide(); 


    }
else
{
 $("#BSC1").hide();   
 $("#BSC2").show(); 

}

    oldValue=value
 }
 
 
 
 setInterval(transferValue,333) //3 times per second



//.side1.multiple
</SCRIPT>

