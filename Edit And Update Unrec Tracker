  Parameters:
  table : IFL_UNREC_TRACKER
  
  -------------------------------------------------------------------------------
  Iron Python (Update btn)
  -------------------------------------------------------------------------------
  import clr
clr.AddReference("System.Windows.Forms")
from System.Windows.Forms import MessageBox
from Spotfire.Dxp.Data import DataPropertyClass
from Spotfire.Dxp.Data import DataTable
from Spotfire.Dxp.Data import DataValueCursor
from System.Collections import IEnumerable
from Spotfire.Dxp.Application.Scripting import ScriptDefinition
from System.Collections.Generic import List, Dictionary
#MessageBox.Show("Do you want to update record?")
Document.Properties["markedaccrecid"]=""
selACCOUNT_REC_ID = DataValueCursor.CreateFormatted(table.Columns["Account Rec ID"])
markedRows = Document.Data.Markings["Update"].GetSelection(table).AsIndexSet()
tblgetrows = IEnumerable.GetEnumerator(table.GetRows(markedRows, selACCOUNT_REC_ID))

try:
		for row in tblgetrows:
			Document.Properties["markedaccrecid"]=selACCOUNT_REC_ID.CurrentValue
			Document.Properties["iaccrecid"]=selACCOUNT_REC_ID.CurrentValue
			
finally:
		tblgetrows.Dispose()

if (Document.Properties["markedaccrecid"].strip()==""):
       Document.Properties["Messagetext"]="Mark at least one record to update"
else:
     Document.Properties["pHelp"]=Document.Properties["ihelp"].replace("'","~&~")
     Document.Properties["pDriverNme"]=Document.Properties["IDriverNme"]
     Document.Properties["pReasonForUnrec"]=Document.Properties["IReasonForUnrec"].replace("'","~&~")
     Document.Properties["pActionPlan"]=Document.Properties["IActionPlan"].replace("'","~&~")
     Document.Properties["pExpectedCloseDt"]=Document.Properties["iExpectedCloseDt"]
     Document.Properties["pStatusCd"]=Document.Properties["iStatusCd"]
     Document.Properties["pEstPIImpactAmt"]=Document.Properties["iEstPlImpactAmt"]
     Document.Properties["pRecTeam"]=Document.Properties["irecteam"].replace("'","~&~")
     Document.Properties["pAccntMngr"]=Document.Properties["iAccntMngr"].replace("'","~&~")
     Document.Properties["paccleader"]=Document.Properties["Iaccleader"].replace("'","~&~")
     Document.Properties["ppendingaction"]=Document.Properties["Ipendingaction"].replace("'","~&~")
     Document.Properties["pExpectPerRecon"]=Document.Properties["iExpectPerRecon"]
     Document.Properties["pPLImpact"]=Document.Properties["iPLImpact"].replace("'","~&~")
     dataTable.Refresh()
     print("success")
     Document.Properties["IDriverNme"]=""
	 
     Document.Properties["IReasonForUnrec"]=""
     Document.Properties["IActionPlan"]=""
     Document.Properties["iExpectedCloseDt"]=None
     Document.Properties["iStatusCd"]=""
     Document.Properties["iEstPlImpactAmt"]=""
     Document.Properties["irecteam"]=""
     Document.Properties["iAccntMngr"]=""
     Document.Properties["ihelp"]=""
     Document.Properties["Iaccleader"]=""
     Document.Properties["Ipendingaction"]=""
     Document.Properties["iExpectPerRecon"]=""
     Document.Properties["iPLImpact"]=""
     Document.Properties["Messagetext"]="Success."

------------
Parameters:
table : IFL_UNREC_TRACKER (Main)
dataTable : IFL_UNREC_MULT_ROWS_NEW (data-on-demand)
vis : Table visual

----------------------------------------------------------------------------------------------------------
Stored Procedure to Update
---------------------------------------

create or replace Procedure P_R2r_Unrec_Comments_Update_v1(
i_src_acctId_period IN VARCHAR,
I_Driver_Nme In Varchar,
I_Reason_For_Unrec In Varchar,
i_Update_By_Sso IN Varchar,
I_Action_Plan In Varchar,
i_Expected_Close_Dt IN Date,
I_Status_Cd In Varchar,
I_Est_Pl_Impact_Amt In Varchar,
I_Rec_Team_Owner_Nme In Varchar,
I_Dependency_Desc In Varchar,
I_Accountable_Leader_Nme In Varchar,
i_Pending_actions_nbr In Varchar,
i_Expected_Period_Reconciled In Varchar,
i_accountable_mgr_nme In Varchar,
i_pl_impact In Varchar --Added on 03/15
) As
Begin
 Merge Into R2r_Ods.T_R2r_Unrec_Comments A
Using ( Select SUBSTR(trim(regexp_substr(i_src_acctId_period, '[^,]+', 1, LEVEL)),-(case when substr(trim(regexp_substr(I_Src_Acctid_Period, '[^,]+', 1, LEVEL)),-11)='GEHC_Manual' then 11 else 3 end )) source_cd,
substr(Trim(Regexp_Substr(i_src_acctId_period, '[^,]+', 1, Level)),1,length(Trim(Regexp_Substr(i_src_acctId_period, '[^,]+', 1, Level)))-(case when substr(trim(regexp_substr(I_Src_Acctid_Period, '[^,]+', 1, LEVEL)),-11)='GEHC_Manual' then 19 else 11 end )) account_rec_id,
Substr(Trim(Regexp_Substr(i_Src_Acctid_Period, '[^,]+', 1, Level)),-(case when substr(trim(regexp_substr(I_Src_Acctid_Period, '[^,]+', 1, LEVEL)),-11)='GEHC_Manual' then 18 else 10 end ),6) Period_Id,
I_Driver_Nme Driver_Nme,I_Reason_For_Unrec Reason_For_Unrec,I_Update_By_Sso Update_By_Sso,I_Action_Plan Action_Plan,I_Expected_Close_Dt Expected_Close_Dt,I_Status_Cd Status_Cd,I_Est_Pl_Impact_Amt Est_Pl_Impact_Amt,i_REC_TEAM_OWNER_NME REC_TEAM_OWNER_NME,i_DEPENDENCY_DESC DEPENDENCY_DESC,I_Accountable_Leader_Nme Accountable_Leader_Nme,i_Pending_actions_nbr Pending_actions_nbr
,i_Expected_Period_Reconciled Expected_Period_Reconciled ,i_accountable_mgr_nme as accountable_mgr_nme, i_pl_impact as pl_impact
From Dual
Connect By Instr(i_src_acctId_period, ',', 1, Level - 1) > 0) B
on
(
A.Source_Cd=B.Source_Cd And 
a.Account_Rec_Id =b.Account_Rec_Id and 
A.Period_Id =B.Period_Id
)
When Matched Then Update 
set a.Driver_Nme=	b.Driver_Nme,
a.Reason_For_Unrec=	b.Reason_For_Unrec,
a.Update_By_Sso=	b.Update_By_Sso,
a.Action_Plan=	b.Action_Plan,
a.Expected_Close_Dt=	b.Expected_Close_Dt,
A.Status_Cd=	B.Status_Cd,
A.Est_Pl_Impact_Amt=	B.Est_Pl_Impact_Amt,
A.Etl_Modified_Dt=Sysdate,
A.Etl_Modified_By=B.Update_By_Sso,
A.Rec_Team_Owner_Nme=B.Rec_Team_Owner_Nme,
A.Dependency_Desc=B.Dependency_Desc,
--A.Accountable_Leader_Nme=b.Accountable_Leader_Nme,
A.Pending_Actions_Nbr=B.Pending_Actions_Nbr
,A.Expected_Period_Reconciled=B.Expected_Period_Reconciled,
A.accountable_mgr_nme = B.accountable_mgr_nme,
A.pl_impact = b.pl_impact

When Not Matched Then Insert (Source_Cd, Account_Rec_Id, Period_Id, Driver_Nme, Reason_For_Unrec, Update_By_Sso, Action_Plan, Expected_Close_Dt, Status_Cd, Est_Pl_Impact_Amt,Etl_Created_Dt,Etl_Created_By,Etl_Delete_Ind,Etl_Current_Ind,Etl_Start_Dt,Etl_End_Dt,Rec_Team_Owner_Nme,Dependency_Desc,
--Accountable_Leader_Nme,
Pending_Actions_Nbr
,Expected_Period_Reconciled,accountable_mgr_nme,pl_impact
)
Values (B.Source_Cd, B.Account_Rec_Id, B.Period_Id,B.Driver_Nme,B.Reason_For_Unrec,B.Update_By_Sso,B.Action_Plan,B.Expected_Close_Dt,B.Status_Cd,B.Est_Pl_Impact_Amt,Sysdate,'P_R2R_UNREC_COMMENTS_UPDATE','N','Y',Sysdate,Sysdate,B.Rec_Team_Owner_Nme,B.Dependency_Desc,
--B.Accountable_Leader_Nme,
B.Pending_Actions_Nbr
,B.Expected_Period_Reconciled,B.accountable_mgr_nme,b.pl_impact
);
Commit;
End
P_R2r_Unrec_Comments_Update_v1;
























