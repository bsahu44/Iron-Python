<script type="text/javascript">

$(function() {
//$('.countryLabel').css( "background-color", "" );
});

//$(".country").animate({width:'600px'}, 300);

$( ".menu" ).click(function() {  
$( this ).toggleClass( "active" );
if($(this).hasClass("active"))
{
$("#panel").animate({left:'0px'}, 300);
document.getElementById('panel4').style.display = "none";
}
else
{
$("#panel").animate({left:'-200px'}, 300);
document.getElementById('panel').style.display = "inline";
document.getElementById('panel4').style.display = "inline";
}


});
//.side1.multiple
//.side1.multiple
$(function() {
$('.countryLabel').css( "background-color", "" );
});

$(".country").animate({width:'600px'}, 300);

$( ".menu4" ).click(function() {  
$( this ).toggleClass( "active4" );
if($(this).hasClass("active4"))
{
$("#panel4").animate({left:'0px'}, 100);
document.getElementById('panel').style.display = "none";
}
else
{
$("#panel4").animate({left:'-1010px'},50);
document.getElementById('panel').style.display = "inline";
document.getElementById('panel4').style.display = "inline";
}


});
</script>


<script type="text/javascript">
  function AllowNumbersOnly(e) {
    var code = (e.which) ? e.which : e.keyCode;
    if (code > 31 && (code < 48 || code > 57)) {
      e.preventDefault();
    }
  }
</script>

